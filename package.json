{
   "name": "collections_v2-server",
   "version": "1.0.0",
   "description": "",
   "main": "dist/server.js",
   "scripts": {
      "build": "npx tsc",
      "start": "node dist/server.js",
      "dev": "concurrently \"npx tsc -w\" \"nodemon src/server.ts\"",
      "migrate-latest": "knex --knexfile src/lib/db/knexfile.ts migrate:latest",
      "migrate-rollback": "knex --knexfile src/lib/db/knexfile.ts migrate:rollback",
      "test": "echo \"Error: no test specified\" && exit 1",
      "prepare": "husky install"
   },
   "author": "mannyv123",
   "license": "MIT",
   "lint-staged": {
      "**/*.{ts,json}": [
         "eslint . --fix",
         "prettier --write ."
      ]
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.4.1",
      "express": "^4.18.2",
      "knex": "^3.1.0",
      "mysql2": "^3.9.1"
   },
   "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/node": "^20.11.5",
      "@typescript-eslint/eslint-plugin": "^6.19.0",
      "@typescript-eslint/parser": "^6.19.0",
      "concurrently": "^8.2.2",
      "eslint": "^8.56.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-prettier": "^5.1.3",
      "husky": "^8.0.0",
      "lint-staged": "^15.2.0",
      "nodemon": "^3.0.3",
      "prettier": "^3.2.4",
      "ts-node": "^10.9.2",
      "typescript": "^5.3.3"
   }
}
